{
  "name": "eslint-config-lean",
  "version": "0.0.0",
  "description": "TODO",
  "keywords": [
    "eslint",
    "eslintconfig"
  ],
  "homepage": "https://github.com/kripod/eslint-config-lean#readme",
  "bugs": "https://github.com/kripod/eslint-config-lean/issues",
  "repository": "kripod/eslint-config-lean",
  "license": "MIT",
  "author": "Kristóf Poduszló (https://www.kripod.dev/)",
  "type": "module",
  "exports": {
    "./base": {
      "import": {
        "types": "./dist/esm/base.d.ts",
        "default": "./dist/esm/base.js"
      },
      "require": {
        "types": "./dist/commonjs/base.d.ts",
        "default": "./dist/commonjs/base.js"
      }
    },
    "./node": {
      "import": {
        "types": "./dist/esm/node.d.ts",
        "default": "./dist/esm/node.js"
      },
      "require": {
        "types": "./dist/commonjs/node.d.ts",
        "default": "./dist/commonjs/node.js"
      }
    }
  },
  "files": [
    "dist/"
  ],
  "scripts": {
    "prepare": "tshy",
    "clean": "git clean --interactive -dx --exclude node_modules/",
    "lint": "pnpm run \"/^lint:(?!fix)/\"",
    "lint:format": "prettier --check .",
    "lint:types": "tsc --noEmit",
    "lint:fix": "pnpm run \"/^lint:fix:/\"",
    "lint:fix:format": "prettier --write ."
  },
  "dependencies": {
    "confusing-browser-globals": "^1.0.11"
  },
  "devDependencies": {
    "@changesets/changelog-github": "0.5.0",
    "@changesets/cli": "2.27.8",
    "@total-typescript/tsconfig": "1.0.4",
    "@types/confusing-browser-globals": "1.0.3",
    "eslint": "9.11.0",
    "globals": "15.9.0",
    "prettier": "3.3.3",
    "tshy": "3.0.2",
    "typescript": "5.6.2"
  },
  "peerDependencies": {
    "eslint": ">=9.11"
  },
  "packageManager": "pnpm@9.11.0",
  "tshy": {
    "exports": {
      "./base": "./src/base.ts",
      "./node": "./src/node.ts"
    }
  }
}
